<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.snm.recyclerviewdemo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecyclerViewActivity"></activity>
        <activity android:name=".TabLayoutActivity"></activity>
        <activity android:name=".AboutActivity"></activity>
        <activity android:name=".TextInputLayoutActivity"></activity>
        <activity android:name=".FloatingActionButtonActivity"></activity>
        <activity android:name=".NavigationViewActivity"></activity>
    </application>

</manifest>
